apiVersion: v1
kind: Service
metadata:
  labels:
    app: <%= service_name %>
  name: <%= service_name %>
spec:
  ports:
    - name: 8080-8080
      port: 8080
      protocol: TCP
      targetPort: 8080
  selector:
    app: <%= service_name %>
    role: backend
  sessionAffinity: None
  type: ClusterIP
